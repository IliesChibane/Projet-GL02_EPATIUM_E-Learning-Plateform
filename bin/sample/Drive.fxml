<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="658.0" prefWidth="891.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DriveControleur">
    <children>
        <BorderPane prefHeight="658.0" prefWidth="890.0">
            <top>
                <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <children>
                        <VBox alignment="CENTER" prefHeight="100.0" prefWidth="338.0">
                            <children>
                                <Label prefHeight="55.0" prefWidth="247.0" text="Mes cours" textFill="#82c46c">
                                    <font>
                                        <Font name="Arial Rounded MT Bold" size="38.0" />
                                    </font>
                                </Label>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER" prefHeight="100.0" prefWidth="444.0" />
                    </children>
                </HBox>
            </top>
            <left>
                <VBox alignment="TOP_CENTER" prefHeight="599.0" prefWidth="262.0" BorderPane.alignment="CENTER">
                    <children>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" />
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Button mnemonicParsing="false" onAction="#downloadFile" prefHeight="45.0" prefWidth="250.0" styleClass="paneverte" stylesheets="@style.css" text="Télécharger du serveur">
                                    <font>
                                        <Font name="Arial Rounded MT Bold" size="16.0" />
                                    </font>
                                </Button>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Button mnemonicParsing="false" onAction="#chooseFile" prefHeight="45.0" prefWidth="250.0" styleClass="paneverte" stylesheets="@style.css" text="Télécharger vers le serveur">
                                    <font>
                                        <Font name="Arial Rounded MT Bold" size="16.0" />
                                    </font>
                                </Button>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" />
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" />
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" />
                    </children>
                </VBox>
            </left>
            <center>
                <TableView fx:id="table" prefHeight="558.0" prefWidth="460.0" BorderPane.alignment="CENTER">
                    <columns>
                        <TableColumn id="icon" fx:id="icon" prefWidth="201.0" text="Type" />
                        <TableColumn id="name" fx:id="name" prefWidth="306.0" text="Fichiers" />
                    </columns>
                </TableView>
            </center>
            <right>
                <VBox prefHeight="558.0" prefWidth="150.0" BorderPane.alignment="CENTER" />
            </right>
        </BorderPane>
    </children>
</AnchorPane>
