<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.NotesEnseignantController">
   <children>
      <Label layoutX="14.0" layoutY="22.0" text="Les notes de l'enseignant" />
      <ComboBox fx:id="section" layoutX="73.0" layoutY="79.0" onAction="#SetupModuleCombo" prefWidth="150.0" />
      <ComboBox fx:id="module" layoutX="380.0" layoutY="79.0" onAction="#SetupTableNote" prefWidth="150.0" />
      <TableView fx:id="TableNote" layoutX="36.0" layoutY="135.0" prefHeight="200.0" prefWidth="524.0">
        <columns>
          <TableColumn fx:id="MatC" prefWidth="75.0" text="Matricule" />
          <TableColumn fx:id="NomC" prefWidth="75.0" text="Nom" />
            <TableColumn fx:id="PrenomC" prefWidth="75.0" text="Prenom" />
            <TableColumn fx:id="ExamC" onEditCommit="#Notecours" prefWidth="75.0" text="Exam" />
            <TableColumn fx:id="TDC" onEditCommit="#NoteTd" prefWidth="75.0" text="TD" />
            <TableColumn fx:id="TPC" onEditCommit="#NoteTP" prefWidth="75.0" text="TP" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="274.0" layoutY="353.0" mnemonicParsing="false" onAction="#Noter" text="Noter" />
   </children>
</AnchorPane>
